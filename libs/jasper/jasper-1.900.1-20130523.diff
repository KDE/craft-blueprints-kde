+    ${CMAKE_SOURCE_DIR}/jpc/jpc_mqdec.c
+    ${CMAKE_SOURCE_DIR}/jpc/jpc_mqenc.c
+    ${CMAKE_SOURCE_DIR}/jpc/jpc_qmfb.c
